<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
    <option name="COLLAPSE_FILE_HEADER" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="LINE_SPACING" value="1.5" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="PHP" value="true" />
        <entry key="Twig" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo\b:.*">
      <option name="FOREGROUND" value="2320dd" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="2320dd" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bfixme\b:.*">
      <option name="FOREGROUND" value="db1715" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="db1715" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bdebug\b:.*">
      <option name="FOREGROUND" value="515151" />
      <option name="BACKGROUND" value="ffc57a" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ffc57a" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bmissing\b:.*">
      <option name="FOREGROUND" value="efefef" />
      <option name="BACKGROUND" value="c943d1" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="c943d1" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bold\b:.*">
      <option name="FOREGROUND" value="efefef" />
      <option name="BACKGROUND" value="a4162" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="a4162" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bmaybe\b:.*">
      <option name="FOREGROUND" value="5787ff" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="5787ff" />
    </pattern>
  </component>
  <component name="XmlFoldingSettings">
    <option name="COLLAPSE_HTML_STYLE_ATTRIBUTE" value="false" />
    <option name="COLLAPSE_ENTITIES" value="false" />
    <option name="COLLAPSE_DATA_URI" value="false" />
  </component>
</application>